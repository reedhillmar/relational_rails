<%= form_with url: "/artists/#{@artist.id}/albums", method: :post, data: {turbo: false} do |form| %>
  <%= form.label :title, "Album Title:" %><br>
  <%= form.text_field :title %><br>
  <%= form.label :number_of_songs, "Number of Songs:" %><br>
  <%= form.text_field :number_of_songs %><br>
  <%= form.label :year_released, "Year Released:" %><br>
  <%= form.text_field :year_released %><br>
  <%= form.label :ep, "EP?" %><br>
  <%= form.check_box :ep %><br>

  <%= form.submit "Create Album"%>
<% end %>