<%= form_with url: "/artists/#{@artist.id}", method: :patch, data: {turbo: false} do |form| %>
  <%= form.label :name, "Artist Name:" %><br>
  <%= form.text_field :name, {:value => @artist.name} %><br>
  <%= form.label :year_formed, "Year Formed:" %><br>
  <%= form.text_field :year_formed, {:value => @artist.year_formed} %><br>
  <%= form.label :touring, "Touring?" %><br>
  <%= form.check_box :touring, {checked: @artist.touring} %><br>
  <%= form.submit "Update Artist" %>
<% end %>