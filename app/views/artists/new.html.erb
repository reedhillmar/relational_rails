<form action="/artists" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <label for="name">Artist Name:</label><br>
  <input type="text" id="name" name="name"><br>
  <label for="year_formed">Year Formed:</label><br>
  <input type="text" id="year_formed" name="year_formed"><br>
  <label for="touring">Touring?</label><br>
  <input type="checkbox" id="touring" name="touring"><br>
  <input type='submit' value="Create Artist"/>
</form>