<%= form_with url: "/artists", method: :post, data: {turbo: false} do |form| %>
  <%= form.label :name, "Artist Name:" %><br>
  <%= form.text_field :name %><br>
  <%= form.label :year_formed, "Year Formed:" %><br>
  <%= form.text_field :year_formed %><br>
  <%= form.label :touring, "Touring?" %><br>
  <%= form.check_box :touring %><br>
  <%= form.submit "Create Artist" %>
<% end %>